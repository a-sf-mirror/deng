				<Compiler>
					<Add option="/MTd" />
					<Add option="/Od" />
					<Add option="/Wp64" />
					<Add option="/W3" />
					<Add option="/ZI" />
					<Add option="/D_CRT_SECURE_NO_DEPRECATE" />
					<Add option="/D__JDOOM__" />
					<Add option="/DWIN32" />
					<Add option="/D_DEBUG" />
					<Add option="/D_WINDOWS" />
					<Add option="/D_USRDLL" />
					<Add option="/DJDOOM_EXPORTS" />
					<Add directory="..\..\plugins\jdoom\include" />
					<Add directory="..\..\plugins\common\include" />
					<Add directory="..\..\engine\api" />
					<Add directory="..\..\external\lzss\portable\include" />
				</Compiler>
				<ResourceCompiler>
					<Add directory="..\..\plugins\jdoom\include" />
					<Add directory="..\..\plugins\common\include" />
					<Add directory="..\..\engine\api" />
					<Add directory="..\..\external\lzss\portable\include" />
				</ResourceCompiler>
				<Linker>
					<Add option='/DEF:&quot;..\..\plugins\jdoom\api\jdoom.def&quot;' />
					<Add option="/opt:noicf" />
					<Add option="/opt:noref" />
					<Add library="bin\Debug Win32\Doomsday.lib" />
					<Add library="lzss" />
					<Add directory="..\..\external\lzss\win32" />
				</Linker>
			</Target>
		</Build>

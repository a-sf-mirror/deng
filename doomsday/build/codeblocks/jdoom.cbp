	<Project>
		<Option title="jDoom" />
		<Option pch_mode="2" />
		<Option compiler="msvc8" />
		<Build>
			<Target title="Release Win32">
				<Option platforms="Windows;" />
				<Option output="bin\Release Win32\jDoom" prefix_auto="1" extension_auto="1" />
				<Option object_output=".\Obj\Release Win32\jDoom" />
				<Option type="3" />
				<Option compiler="msvc8" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="/MT" />
					<Add option="/GA" />
					<Add option="/fp:except" />
					<Add option="/GF" />
					<Add option="/Ox" />
					<Add option="/W3" />
					<Add option="/Ob1" />
					<Add option="/D__JDOOM__" />
					<Add option="/DWIN32" />
					<Add option="/DNDEBUG" />
					<Add option="/D_WINDOWS" />
					<Add option="/D_USRDLL" />
					<Add option="/DJDOOM_EXPORTS" />
					<Add directory="..\..\plugins\jdoom\include" />
					<Add directory="..\..\plugins\common\include" />
					<Add directory="..\..\engine\api" />
					<Add directory="..\..\external\lzss\portable\include" />
				</Compiler>
				<ResourceCompiler>
					<Add directory="..\..\plugins\jdoom\include" />
					<Add directory="..\..\plugins\common\include" />
					<Add directory="..\..\engine\api" />
					<Add directory="..\..\external\lzss\portable\include" />
				</ResourceCompiler>
				<Linker>
					<Add option="/INCREMENTAL:NO" />
					<Add option='/DEF:&quot;..\..\plugins\jdoom\api\jdoom.def&quot;' />
					<Add library="bin\Release Win32\Doomsday.lib" />
					<Add library="lzss" />
					<Add directory="..\..\external\lzss\win32" />
				</Linker>
			</Target>
			<Target title="Debug Win32">
				<Option platforms="Windows;" />
